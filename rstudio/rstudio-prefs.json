{
    "jobs_tab_visibility": "shown",
    "initial_working_directory": "~",
    "python_type": "virtualenv",
    "python_version": "3.13.2",
    "python_path": "/opt/homebrew/bin/python3",
    "posix_terminal_shell": "bash",
    "default_project_location": "~/Developer",
    "show_margin": false,
    "rmd_viewer_type": "pane",
    "num_spaces_for_tab": 4,
    "auto_detect_indentation": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "font_size_points": 9,
    "editor_line_height": 140,
    "text_rendering": "geometricPrecision",
    "soft_wrap_r_files": true,
    "editor_theme": "Cobalt"
}